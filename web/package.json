{
  "name": "gnet",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/classnames": "^2.2.7",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.13.4",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.2",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "jquery": "^3.4.0",
    "lodash": "^4.17.11",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "popper": "^1.0.1",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^2.1.8",
    "redux": "^4.0.1",
    "redux-starter-kit": "^0.4.3",
    "stylelint": "^9.10.1"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "./node_modules/.bin/prettier --single-qute --write --parser \"typescript\",",
      "git add"
    ]
  },
  "scripts": {
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "./node_modules/.bin/react-scripts start",
    "start": "./node_modules/.bin/npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "./node_modules/.bin/react-scripts test",
    "eject": "./node_modules/.bin/react-scripts eject",
    "lint": "tslint -c tslint.json src/*.tsx src/*/*.tsx src/*/*/*.tsx"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "react-docgen-typescript": "^1.12.3",
    "react-styleguidist": "^9.0.7",
    "redux-devtools": "^3.5.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
